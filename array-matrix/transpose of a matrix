#include <stdio.h>

int main() {
    int m,n,i,j;
    printf("ENTER THE ROWS AND COLUMN OF MATRIX:\n");
    scanf("%d%d",&m,&n);
    
    int a[m][n], b[n][m];
    printf("ENTER THE ELEMENT OF MATRIX:\n");
    for(i=0; i<m; i++){
        for(j=0; j<n; j++){
            scanf("%d",&a[i][j]);
        }
    }
    printf("THE ELEMENT OF MATXIX ARE:\n");
    for(i=0; i<m; i++){
        for(j=0; j<n; j++){
            printf("%d\t",a[i][j]);
        }
        printf("\n");
    }
    
    for(i=0; i<m; i++){
        for(j=0; j<n; j++){
            b[j][i]=a[i][j];
        }
    }
    printf("THE TRANSPOSE OF MATXIX ARE:\n");
    for(i=0; i<m; i++){
        for(j=0; j<n; j++){
            printf("%d\t",b[i][j]);
        }
        printf("\n");
    }
    
    return 0;
}
