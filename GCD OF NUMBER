#include<stdio.h>
int main()
{
    int num1,num2,GCD;
    
    printf("ENTER THE FIRST NUMBER:");
    scanf("%d",&num1);
    printf("ENTER THE SECOND NUMBER:");
    scanf("%d",&num2);
    
    for(int i=1; i<=num1 && i<=num2; i++)
    {
        if(num1%i==0 && num2%i==0)
        GCD=i;
    }
    printf("%d is GCD of num1 & num2",GCD);
    
    return 0;
}
