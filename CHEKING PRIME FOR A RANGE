#include <stdio.h>
#include<math.h>

int main() {
    int num;
    printf("ENTER THE TERMS:");
    scanf("%d",&num);
    
    for(int i=2;i<=num;i++){
        int flag=1;
        for(int j=2; j<=sqrt(i); j++){
            if(i%j==0){
            flag=0;
            break;
            }
        }
        if(flag==0)
        printf("%d is not prime number\n",i);
        
        else
        printf("%d is prime number\n",i);
    }
    return 0;
}

