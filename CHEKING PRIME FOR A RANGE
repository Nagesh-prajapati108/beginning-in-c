#include<stdio.h>
int main()
{   
    for(int n=2; n<=100; n++){
        int a=1;
        for(int i=2; i<=n/2; i++){
            if(n % i==0){
            a=0;
            break;
        }
    }
    if(a==0)
        printf("%d is not a prime number\n" , n);
    else
        printf("%d is prime number\n", n);
    }
    return 0;
}
