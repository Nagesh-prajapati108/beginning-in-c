#include<stdio.h>
int main()
{
    int n, a=0;
    printf("ENTER THE NUMBER:");
    scanf("%d",&n);
    
    if(n<0)
    printf("not a prime number");
    
    else
    {
        for(int i=2; i<n; i++)
        {
            if(n%i==0)
            a=1;
        }
    
    if(a==1)
        printf("not a prime number");
    else
        printf("prime number");
    }
    
    return 0;
    
}
